{
    "name": "progresivjose/piano-oauth",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.7"
    },
    "require-dev": {
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-mock": "^2.0",
        "pestphp/pest-plugin-faker": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Progresivjose\\PianoOauth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "José Manuel González Ricart",
            "email": "jmanuelgricart@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
